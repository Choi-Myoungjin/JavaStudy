package test1;

public class Student extends PersonInfo {
	int level, num;
	String major;

	Student(String name, String address, int num, int level, String major) {
		super(name, address);
		this.level = level;
		this.major = major;
		this.num = num;

	}

	public int getLevel() {
		return level;
	}

	public int getNum() {
		return num;
	}

	public String getMajor() {
		return major;
	}

	public void StudentPrint() {
		// for(int i = 0 ; i < Student.lenght; i++)
		// {
		// System.out.println(getName());
		// }
		System.out.println(
				getName() + "  /  " + getAddress() + "  /  " + getNum() + "  /  " + getLevel() + "  /  " + getMajor());
	}
}
